  },
  {
   "compiler": "XE4",
   "platforms": "Win32, Win64",
   "template": "default"
  },
  {
   "compiler": "XE3",
   "platforms": "Win32, Win64",
   "template": "default"
  },
  {
   "compiler": "XE2",
   "platforms": "Win32, Win64",
   "template": "default"
  },
  {
   "compiler": "13.0",
   "platforms": "Win32, Win64",
   "template": "12plus"
  }
 ],
 "templates": [
  {
   "name": "12plus",
   "source": [
    {
     "src": "src\\**.pas",
     "dest": "src",
     "flatten": true
    },
    {
     "src": "packages\\Rad Studio $Compiler$\\*.dp*",
     "dest": "packages\\Rad Studio $Compiler$",
     "exclude": [
      "*.dproj.local"
     ]
    }
   ],
   "searchPaths": [
    {
     "path": "src"
    }
   ],
   "build": [
    {
     "id": "Runtime",
     "project": ".\\packages\\Rad Studio $compiler$\\VSoft.CommandLineR.dproj",
     "config": "Release"
    }
   ]
  },
  {
   "name": "default",
   "source": [
    {
     "src": "src\\**.pas",
     "dest": "src",
     "flatten": true
    },
    {
     "src": "packages\\Rad Studio $CompilerWithCodeName$\\*.dp*",
     "dest": "packages\\Rad Studio $CompilerWithCodeName$",
     "exclude": [
      "*.dproj.local"
     ]
    }
   ],
   "searchPaths": [
    {
     "path": "src"
    }
   ],
   "build": [
    {
     "id": "Runtime",
     "project": ".\\packages\\Rad Studio $compilerWithCodeName$\\VSoft.CommandLineR.dproj",
     "config": "Release"
    }
   ]
  }
 ]
}
